<h1>Liste des droits</h1>
<%= link_to 'Créer un nouveau droit', new_admin_user_droit_path %>
<table>
	<tr>
		<td>
			intitulé
		</td>
		<td>
			code inchangeable
		</td>
	</tr>
	<% @user_droits.each do |user_droit| %>
	<tr>
		<td>
			<%= user_droit.intitule %>
		</td>
		<td>
			<%= user_droit.code_inchangeable %>
		</td>
	</tr>
	<% end %>
</table>